<script>
    async function cancelAppointment(appointment) {
        var confirmation = prompt(`type 'yes' if you want to cancel appointment with appoinment ID ${appointment}`);
        if (confirmation === "yes") {
            const res = await fetch(`/appointment-cancel/${appointment}`, {
                method: 'POST'
            });
            alert('Appointment cancelled');
            window.location = "/user-dashboard-appointments";
        } else {
            alert('Cancellation aborted')
        }
    }

    function rate(elem) {
        var num = parseInt(elem.getAttribute('data-starCount'));
        var appointmentId = elem.getAttribute('data-appointmentid');
        for (i = 1; i <= num; i++) {
            document.getElementById(`${i}star`).classList.add('checked')
        }
        for (i = 5; i > num; i--) {
            document.getElementById(`${i}star`).classList.remove('checked')
        }
        //alert(elem.getAttribute('data-appointmentid'));
        fetch(`/rate-appointment/${appointmentId}?rating=${num}`, {
                method: 'POST'
            })
            .then(response => response.json())
            .catch(error => alert(`Error: ${error.message}`))
            .then(() => {
                window.location.reload()
            })

    }

    function popupCancelAppointment(button) {
        const popCancel = document.querySelector('.cancel_appointment_form_container');
        popCancel.classList.toggle('d-none');
        console.log(Array.from(popCancel.classList).indexOf('d-none'));
        if (Array.from(popCancel.classList).indexOf('d-none') === -1) {
            popCancel.querySelector('.cancel_appointment_options--cancel').children[0].dataset.appointment = button.dataset.appointment;
        }
    }
</script>

</html>