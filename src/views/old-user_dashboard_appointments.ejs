                                    </div>
                                    <ul class="list-group list-group-flush w-100">
                                        <% appointments.forEach((appointment)=>{%>
                                            <li class="list-group-item text-center w-100">
                                                <% if(appointment.status==="Cancelled"){ %>
                                                    <b class="text-center text-danger">Cancelled</b>
                                                    <% }else{%>
                                                        <% if(appointment.appointmentDate<Date.now()){%>
                                                            <b class="text-center text-danger">Past appointment</b>
                                                            <% if(typeof appointment.rating !== 'undefined' && appointment.rating){%>
                                                                <div>
                                                                    <span class="fa fa-star <%= parseInt(appointment.rating)>0?'checked':'' %>"></span>
                                                                    <span class="fa fa-star <%= parseInt(appointment.rating)>1?'checked':'' %>"></span>
                                                                    <span class="fa fa-star <%= parseInt(appointment.rating)>2?'checked':'' %>"></span>
                                                                    <span class="fa fa-star <%= parseInt(appointment.rating)>3?'checked':'' %>"></span>
                                                                    <span class="fa fa-star <%= parseInt(appointment.rating)>4?'checked':'' %>"></span>
                                                                </div>
                                                                <%}else{%>
                                                                    <div>
                                                                        <b class="d-block text-center text-dark">How was your experience?</b>
                                                                        <span class="pointer fa fa-star" onclick="rate(this)" data-appointmentid="<%= appointment._id %>" data-starCount="1" id="1star"></span>
                                                                        <span class="pointer fa fa-star" onclick="rate(this)" data-appointmentid="<%= appointment._id %>" data-starCount="2" id="2star"></span>
                                                                        <span class="pointer fa fa-star" onclick="rate(this)" data-appointmentid="<%= appointment._id %>" data-starCount="3" id="3star"></span>
