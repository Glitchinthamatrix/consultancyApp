<script>
    function deleteDoctor(doctorId, doctorName) {
        var confirmation = prompt(`Type 'delete' if you want to delete ${doctorName}'s profile`);
        if (confirmation === 'delete') {
            fetch(`/delete-doctor/${doctorId}`, {
                method: 'GET'
            }).then(() => {
                alert('deleted');
                window.location.reload();
            })
        } else {
            alert('deletion aborted')
        }
    }
</script>

